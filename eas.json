{
  "cli": {
    "version": ">= 16.13.3",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_ENV": "dev"
      }
    },
    "uat": {
      "distribution": "internal",
      "env": {
        "APP_ENV": "uat"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "APP_ENV": "prod"
      }
    },
    "preview": {
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}